  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/framework-bundle": "^5.2",
    "symfony/orm-pack": "^1.0",
    "doctrine/annotations": "^1.11",
    "doctrine/orm": "^2.9",
    "mongodb/mongodb": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}