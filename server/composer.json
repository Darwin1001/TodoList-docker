        "php": "^8.2",
        "symfony/framework-bundle": "^6.3",
        "doctrine/mongodb-odm-bundle": "^4.6",
        "ext-mongodb": "^2.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
